public class Write{
	
	private String sBoardName;
	
	public Write() {
		init();
	}
	
	public void init() {
		this.setBoardName("Free Board");
	}
	
	public void setBoardName(String sBoardName) {
		this.sBoardName = sBoardName;
	}
	
	public void getBoardName() {
		return sBoardName;
	}
	
	public void doWrite() {
		System.out.println(this.getsBoardName() + "Wrtie down!!");
	}
}